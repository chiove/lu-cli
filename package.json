    "@babel/preset-react": "^7.10.4",
    "autoprefixer": "^9.8.6",
    "axios": "^0.19.2",
    "babel-eslint": "8",
    "babel-loader": "^8.1.0",
    "babel-polyfill": "^6.26.0",
    "chokidar": "^3.4.2",
    "cross-env": "^7.0.2",
    "css-hot-loader": "^1.4.4",
